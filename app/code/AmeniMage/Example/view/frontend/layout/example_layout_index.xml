<?xml version="1.0"?>
<page  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Layout example</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="page_title" xsi:type="string">Layout Title
                </argument>
            </action>
        </referenceBlock>
       <referenceContainer name="content">
           <block name="layout_example" class="AmeniMage\Example\Block\Example" template="AmeniMage_Example::example.phtml">
                <block name="layout_note" class="AmeniMage\Example\Block\Note" template="AmeniMage_Example::note.phtml"/>
                <block name="cms_block" class="Magento\Cms\Block\Block" >
                    <arguments>
                        <argument name="block_id" xsi:type="string">cms_block_from_admin</argument>
                    </arguments>
                </block>
           </block>
       </referenceContainer>
    </body>
</page>
